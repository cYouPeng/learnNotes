package atm;

public class User {
	static int num;
	String id;
	String pwd;
	String name;
	double money;
	static{
		num = 1000;
	}
	{
		this.id = String.valueOf(++num);
	}
	public User(){
		
		this.pwd = "000000";
	}
	public User(String pwd, String name, double money){
		
		this.pwd = pwd;
		this.name = name;
		this.money = money;
	}
}
class CardIdFactory{
	static int num;
	static{
		num = 1000;
	}
	public String newCardId(){
		return String.valueOf(++num);
	}
}
