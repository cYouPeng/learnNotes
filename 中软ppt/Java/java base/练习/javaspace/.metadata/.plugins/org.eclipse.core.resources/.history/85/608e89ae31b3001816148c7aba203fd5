package a;

public class Test3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = new int[10];
		arr[0] = 1;
		arr[1] = 2;
		for(int i=2; i<arr.length; i++){
			arr[i] = arr[i-1] + arr[i-2];
		}
		int sum = 0;
		for(int num :arr){
			sum += num;
		}
		

		
	}

}
