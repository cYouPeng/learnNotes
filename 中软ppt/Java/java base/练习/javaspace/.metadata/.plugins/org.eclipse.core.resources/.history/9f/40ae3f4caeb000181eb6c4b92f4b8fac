package io;

import java.io.File;

public class TestFindClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String a = "";
		String b = "";
		System.out.println(a==b);
		String uri = TestFindClass.class.getResource("/").getFile();
		//new TestFindClass().findAllFile(new File(uri));
	}
	
	
	public void findAllFile(File root){
		if(!root.isDirectory()){
			return;
		}
		File[] files = root.listFiles();
		for(File f : files){
			
			if(f.isDirectory()){
				this.findAllFile(f);
			}
			if(f.isFile()){
				String name = this.getClassFullName(f);
				System.out.println(name);
			}
		}
	}
	public String getClassFullName(File classFile){
		if(!classFile.getName().endsWith("class")){
			return null;
		}
		String path = classFile.getParent();
		String packageName = path.split("bin\\\\")[1].replaceAll("\\\\", ".");
		String className = classFile.getName().split("\\.")[0];
		return packageName+"."+className;
	}

}
