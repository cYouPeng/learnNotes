package a;

public class Test2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//2-有数列，1+2+3+5+8+...求前10项总和
//		num1 1 2 3
//		temp 2 3 5
		int num1 = 1;
		int temp = 2;
		int sum = 0;
		for(int i=0; i<10; i++){
			sum += num1;
			int a = temp;
			temp = num1+temp;
			num1=a;
		}
	}

}
