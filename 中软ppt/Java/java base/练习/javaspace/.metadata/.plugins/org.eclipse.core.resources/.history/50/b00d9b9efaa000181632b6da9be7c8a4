package string;

public class Lianxi7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "ABCabcdefabcdefgh";
		int[] arr = new int[26];
		
		for(int i=0; i<s.length(); i++){
			char c = s.charAt(i);
			if(c>='A'&&c<='A'+25){
				c+= 'a'-'A';
			}
			arr[c-97]++;
		}
		for(int i=0; i<arr.length; i++){
			System.out.println((char)(i+97)+"出现"+arr[i]+"次");
		}
	}

}
