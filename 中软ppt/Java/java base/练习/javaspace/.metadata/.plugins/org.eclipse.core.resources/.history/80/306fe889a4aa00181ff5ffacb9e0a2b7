package innerclass;

public class Test2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
}
class Outer{
	private int instanceField;
	private static int staticField;
	
	/**
	 * 成员内部类,可以访问外部类的所有（静态或实例）成员
	 * @author Administrator
	 *
	 */
	class Inner{
		public void method(){
			instanceField = 10;
			staticField = 20;
		}
	}
	/**
	 * 静态内部类
	 * @author Administrator
	 *
	 */
	static class StaticInner{
		public void method(){
			instanceField = 10;
			staticField = 20;
		}
	}
	
}