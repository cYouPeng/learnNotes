package oop1;

public class Test3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		int b = 20;
		Test3 t = new Test3();
		t.swap(a, b);
		System.out.println("a="+a+",b="+b);
	}
	
	public void swap(int a, int b){
		int temp = a;
		a = b;
		b = temp;
	}
	
	public void swap(int[] arr){
		for(int i=0, j=arr.length-1; i<j; i++,j--){
			int temp = arr[i];
			arr[i] = arr[j];
			arr[j] = temp;
		}
	}

}
