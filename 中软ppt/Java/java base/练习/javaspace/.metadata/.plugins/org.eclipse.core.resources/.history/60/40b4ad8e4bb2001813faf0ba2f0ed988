package thread;

public class TestThread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//mt就是一个县城对象
		MyThread mt = new MyThread();//mt新建状态
		mt.start();//mt转成就绪状态，等待cpu时间片达到运行状态，运行run方法的代码，run方法执行结束mt线程结束
		MyThread mt2 = new MyThread();
		mt2.start();
	}

}

class MyThread extends Thread{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		for(int i=1; i<=10; i++){
			System.out.println(i);
		}
	}
	
}
